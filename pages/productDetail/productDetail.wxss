  height:80rpx;
}
.no-scroll{
  height:100%;
  overflow:hidden;
}
.hidden{
  position:relative;
}
.hidden-btn{
  position:absolute;
  top:0;left:0;
  z-index:100;
  width:100%;height:100%;
  opacity:0;
}


.animate-view{
  position:absolute;
  top:0;right:100rpx;
