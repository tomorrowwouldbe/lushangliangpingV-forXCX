  width:40rpx;
  height:40rpx;
}
.item-2,.item-3{
  width:calc(50% - 120rpx);
  text-align:center;
  border-right:2rpx solid #e5e5e5;
}
.item-3{
  background-color:#cf2122;
  border-right:0;
  color:#fff;
}
.item-2{
  color:#333;
}
.item-2:not(.disabled):active,.item-3:not(.disabled):active{
  opacity:0.5;
}

.choices-alert{
  position:fixed;
  left:0;top:0;
  width:100%;height:100%;
  background-color:rgba(0,0,0,0.5);
