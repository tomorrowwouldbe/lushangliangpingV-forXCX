  height:44rpx;
  margin-top:28rpx;
}
.item-1{
  width:120rpx;
  text-align:center;
  border-right:2rpx solid #e5e5e5;
  position:relative;
}
.cart-num{
  position:absolute;
  right:20rpx;top:20rpx;
  display:inline-block;
  width:30rpx;
  height:30rpx;
