  font-size:30rpx;
  color:#666;
  margin:0 0 10rpx;
}
.product-s-title:first-child{
  margin-top:30rpx;
}
.choose-item{
  display:inline-block;
  padding:0 10rpx;
  height:74rpx;line-height:72rpx;
  border:1rpx solid #666;color:#666;
  margin-right:20rpx;
  margin-bottom:20rpx;
  min-width:130rpx; 
  text-align:center;
  border-radius:8rpx;
  font-size:30rpx;
}
/* .choose-items .choose-item:nth-child(4n){
  margin-right:0;
} */
.choose-item.active{
  border-color:#cf2122;
